<h1>random-beeers</h1>

<h1>beeers</h1>
  {{#each beer as |cerveza|}}
   <h1> {{ cerveza.name }}</h1>
   
   <img src="{{cerveza.image_url}}" alt="">
   <p>{{cerveza.description}}</p>
  <h3>{{cerveza.tagline}}</h3>

  <h3>Food Paring</h3>
  <p>{{cerveza.food_pairing}}</p>

  <h3>Brewers Tips</h3>
  <p>{{cerveza.brewers_tips}}</p>
  {{/each}}

